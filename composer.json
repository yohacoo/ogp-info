{
  "name": "yohacoo/ogp-info",
  "description": "Library for retrieving OGP information",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Yohacoo\\OgpInfo\\": "src/"
    }
  },
  "scripts": {
    "server": [
      "Composer\\Config::disableProcessTimeout",
      "php -S localhost:8000 -t ./test-document-root/"
    ],
    "test": [
      "./vendor/bin/phpunit tests"
    ],
    "clean": [
      "rm -rf ./src/.ogp-cache"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^12"
  }
}
